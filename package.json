  "build": {
    "afterSign": "internals/scripts/notarize.js",
    "generateUpdatesFilesForAllChannels": true,
    "productName": "Auryo",
    "files": [
      "dist/electron/**/*"
    ],
    "appId": "be.sneljo.auryo",
    "publish": [
      {
        "provider": "github",
        "owner": "Superjo149",
        "repo": "auryo"
      }
    ],
    "protocols": [
      {
        "name": "auryo",
        "role": "Viewer",
        "schemes": [
          "auryo"
        ]
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "category": "public.app-category.music"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "category": "Audio",
      "target": [
        "deb",
        "rpm",
        "AppImage",
        "pacman"
      ]
    },
    "snap": {
      "useTemplateApp": false,
      "summary": "A SoundCloud client for your desktop",
      "buildPackages": [
        "execstack"
      ],
      "stagePackages": [
        "libasound2",
        "libgconf2-4",
        "libnotify4",
        "libnspr4",
        "libnss3",
        "libpcre3",
        "libpulse0",
        "libxss1",
        "libxtst6"
      ],
      "plugs": [
        "bluez",
        "browser-support",
        "gsettings",
        "browser-support",
        "gsettings",
        "home",
        "mount-observe",
        "network",
        "opengl",
        "pulseaudio",
        "unity7",
        "x11",
        "mpris"
      ],
      "category": "Audio"
    },
    "directories": {
      "output": "build/release"
    }
  },
  "devEngines": {
    "node": ">=12.x",
    "npm": ">=5.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint -- --fix"
    }
  },
  "resolutions": {
    "**/**/nan": "^2.9.2"
